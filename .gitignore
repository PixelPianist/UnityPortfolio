#################################
# Unity .gitignore
#################################
/unity-projects/*/[Ll]ibrary/
/unity-projects/*/[Tt]emp/
/unity-projects/*/[Oo]bj/
/unity-projects/*/[Bb]uild/
/unity-projects/*/[Bb]uilds/
/unity-projects/*/[Ll]ogs/
/unity-projects/*/[Uu]ser[Ss]ettings/

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/unity-projects/*/[Mm]emoryCaptures/

# Recordings can get excessive in size
/unity-projects/*/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /unity-projects/*/[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/unity-projects/*/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
/unity-projects/*/.vs/

# Gradle cache directory
/unity-projects/*/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
/unity-projects/*/ExportedObj/
/unity-projects/*/.consulo/
/unity-projects/*/*.csproj
/unity-projects/*/*.unityproj
/unity-projects/*/*.sln
/unity-projects/*/*.suo
/unity-projects/*/*.tmp
/unity-projects/*/*.user
/unity-projects/*/*.userprefs
/unity-projects/*/*.pidb
/unity-projects/*/*.booproj
/unity-projects/*/*.svd
/unity-projects/*/*.pdb
/unity-projects/*/*.mdb
/unity-projects/*/*.opendb
/unity-projects/*/*.VC.db

# Unity3D generated meta files
/unity-projects/*/*.pidb.meta
/unity-projects/*/*.pdb.meta
/unity-projects/*/*.mdb.meta

# Unity3D generated file on crash reports
/unity-projects/*/sysinfo.txt

# Builds
/unity-projects/*/*.apk
/unity-projects/*/*.aab
/unity-projects/*/*.unitypackage
/unity-projects/*/*.unitypackage.meta
/unity-projects/*/*.app

# Crashlytics generated file
/unity-projects/*/crashlytics-build.properties

# Packed Addressables
/unity-projects/*/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
/unity-projects/*/[Aa]ssets/[Ss]treamingAssets/aa.meta
/unity-projects/*/[Aa]ssets/[Ss]treamingAssets/aa/*


#################################
# React .gitignore
#################################
/website/.DS_*
/website/*.log
/website/logs
/website/**/*.backup.*
/website/**/*.back.*

/website/node_modules
/website/bower_components

/website/*.sublime*

/website/psd
/website/thumb
/website/sketch

#################################
# Jetbrains .gitignore
#################################
.idea

#################################
# AWS CDK .gitignore
#################################
/infrastructure/*.js
/infrastructure/!jest.config.js
/infrastructure/*.d.ts
/infrastructure/node_modules

# CDK asset staging directory
/infrastructure/.cdk.staging
/infrastructure/cdk.out

#################################
# Kup .gitignore
#################################
/kup/node_modules